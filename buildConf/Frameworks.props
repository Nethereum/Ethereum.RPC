<Project>
  <PropertyGroup>
    <NoWarn>$(NoWarn);NU1701</NoWarn>
    <TargetNet35>true</TargetNet35>
    <TargetNetCore Condition="'$(MSBuildRuntimeType)' == 'Core'">true</TargetNetCore>
    <NetStandardVersion>netstandard2.0</NetStandardVersion>
    <DefaultFrameworksUnitySupport Condition=" '$(TargetNet35)' == 'true' ">$(NetStandardVersion);net35;net451</DefaultFrameworksUnitySupport>
    <DefaultFrameworksUnitySupport Condition=" '$(TargetNet35)' == 'false' ">$(NetStandardVersion);net451</DefaultFrameworksUnitySupport>
    <DefaultFrameworksUnitySupport Condition=" '$(TargetNetCore)' == 'true' ">$(NetStandardVersion)</DefaultFrameworksUnitySupport>
    <TargetFramework>$(NetStandardVersion)</TargetFramework>
  </PropertyGroup>
</Project>
